{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "linebreak-style": "off",
    "react/react-in-jsx-scope": "off",
    "max-len": "off",
    "@next/next/no-img-element": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off"
  }
}
